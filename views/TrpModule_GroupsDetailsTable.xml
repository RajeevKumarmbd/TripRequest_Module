<record id ="trpmodule_groupsdetailstable_view_form" model="ir.ui.view">
   <field name="name">Group Details Input Form</field>
   <field name="model">trpmodule.groupsdetailstable</field>
   <field name="arch" type="xml">
         <form>
			<sheet>
                <group col="4">

                    <field name="name"/>
                    <field name="user_name"/>
                    <field name="age"/>
                    <field name="gender"/>
                    <field name="email_id"/>
                    <field name="phone"/>
                    <field name="expense_born_by"/>
                </group>

			</sheet>
	 </form>
   </field>
</record>

<record id="trpmodule_groupsdetailstable_view_tree" model="ir.ui.view">
  <field name="name">Group Details Data List</field>
  <field name="model">trpmodule.groupsdetailstable</field>
  <field name="arch" type="xml">
    <tree>

                    <field name="name"/>
                    <field name="user_name"/>
                    <field name="age"/>
                    <field name="gender"/>
                    <field name="email_id"/>
                    <field name="phone"/>
                    <field name="expense_born_by"/>
    </tree>
    </field>
</record>
