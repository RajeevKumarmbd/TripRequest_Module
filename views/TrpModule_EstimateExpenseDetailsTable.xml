<record id ="trpmodule_estimateexpensedetailstable_view_form" model="ir.ui.view">
   <field name="name">Expense Estimate Details Input Form</field>
   <field name="model">trpmodule.estimateexpensedetailstable</field>
   <field name="arch" type="xml">
         <form>
			<sheet>
                <group col="4">

                    <field name="name"/>
                    <field name="description_route"/>
                    <field name="number_of_pax"/>
                    <field name="qty"/>
                    <field name="est_price"/>
                    <field name="currency"/>
                    <field name="sub_total"/>
                    <field name="sub_total_inr"/>
                </group>

			</sheet>
	 </form>
   </field>
</record>

<record id="trpmodule_estimateexpensedetailstable_view_tree" model="ir.ui.view">
  <field name="name">Expense Estimate Details Data List</field>
  <field name="model">trpmodule.estimateexpensedetailstable</field>
  <field name="arch" type="xml">
    <tree>

                    <field name="name"/>
                    <field name="description_route"/>
                    <field name="number_of_pax"/>
                    <field name="qty"/>
                    <field name="est_price"/>
                    <field name="currency"/>
                    <field name="sub_total"/>
                    <field name="sub_total_inr"/>

    </tree>
    </field>
</record>
